{
  "name": "mrjs",
  "version": "0.2.01",
  "type": "module",
  "description": "an MR first webXR framework",
  "main": "dist/mr.js",
  "homepage": "https://volumetrics.io",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "server": "npx webpack serve",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-serially": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand",
    "test-randomized": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --random",
    "docs": "jsdoc -c jsdoc.config.json",
    "docs-verbose": "jsdoc -c jsdoc.config.json -X",
    "prettier-list": "prettier --list-different \"src/**/*.js\" \"*.js\"",
    "prettier-fix": "prettier --write \"src/**/*.js\" \"*.js\"",
    "lint-list": "eslint \"src/**/*.js\" \"*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" \"*.js\" --fix",
    "format": "npm run prettier-fix && npm run lint-fix",
    "check-format": "npm run prettier-list && npm run lint-list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volumetrics-io/mrjs.git"
  },
  "author": "Volumetrics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Volumetrics-io/mrjs/issues"
  },
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "testMatch": [
    "**/__tests__/**/*.test.mjs"
  ],
  "testEnvironment": "node",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.5.3",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "npm": "^10.2.4",
    "style-loader": "^3.3.3",
    "url": "^0.11.3",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@dimforge/rapier3d": "*",
    "prettier": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1",
    "stats.js": "^0.17.0",
    "three": "^0.151.3",
    "troika-three-text": "^0.48.0-unicode.1"
  }
}
