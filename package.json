{
  "name": "mrjs",
  "version": "0.3.0",
  "type": "module",
  "description": "an MR first webXR framework",
  "main": "dist/mr.js",
  "homepage": "https://volumetrics.io",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "server": "npx webpack serve",
    "test-server": "NODE_ENV=testing npx webpack serve",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-serially": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand",
    "test-randomized": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --random",
    "docs": "output_dir='docs' && rm -rf \"$output_dir\" && mkdir -p \"$output_dir\" && output_file=\"${output_dir}/js-api.md\" && { echo '---'; echo 'description: \"Javascript API\"'; echo '---'; echo '# Javascript API'; echo ''; echo '<hr>'; echo ''; } > \"$output_file\" && find src -type f -name '*.js' ! -name '_*' ! -name 'index.js' | while read file; do jsdoc2md \"$file\" >> \"$output_file\" && echo '' >> \"$output_file\" && echo \"<div class='centered'><a href='https://github.com/volumetrics-io/mrjs/edit/main/$file' target='_blank'>Suggest an edit on GitHub for $file</a></div>\" >> \"$output_file\" && { echo ''; echo '<hr>'; echo ''; } >> \"$output_file\"; done",
    "prettier-list": "prettier --list-different \"src/**/*.js\" \"*.js\"",
    "prettier-fix": "prettier --write \"src/**/*.js\" \"*.js\"",
    "lint-list": "eslint \"src/**/*.js\" \"*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" \"*.js\" --fix",
    "format": "npm run prettier-fix && npm run lint-fix",
    "check-format": "npm run prettier-list && npm run lint-list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volumetrics-io/mrjs.git"
  },
  "author": "Volumetrics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Volumetrics-io/mrjs/issues"
  },
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "testMatch": [
    "**/__tests__/**/*.test.mjs"
  ],
  "testEnvironment": "node",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.5.3",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-puppeteer": "^9.0.2",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm": "^10.2.4",
    "playwright": "^1.40.1",
    "puppeteer": "^21.7.0",
    "style-loader": "^3.3.3",
    "tui-jsdoc-template": "^1.2.2",
    "url": "^0.11.3",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@dimforge/rapier3d": "*",
    "docdash": "^2.0.2",
    "jaguarjs-jsdoc": "^1.0.2",
    "jsdom": "^23.2.0",
    "prettier": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1",
    "stats.js": "^0.17.0",
    "three": "^0.151.3",
    "troika-three-text": "^0.48.0-unicode.1"
  }
}
