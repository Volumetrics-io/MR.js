{
  "name": "volumetrics",
  "version": "0.0.1",
  "description": "an MR first webXR framework",
  "main": "dist/build.js",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "npx jsdoc src/*.js --destination ./docs",
    "prettier-list": "prettier --list-different \"src/**/*.js\" \"*.js\"",
    "prettier-fix": "prettier --write \"src/**/*.js\" \"*.js\"",
    "lint-list": "eslint \"src/**/*.js\" \"*.js\"",
    "lint-fix": "eslint \"src/**/*.js\" \"*.js\" --fix",
    "format": "npm run prettier-fix && npm run lint-fix",
    "check-format": "npm run prettier-list && npm run lint-list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Volumetrics-io/volumetrics.git"
  },
  "author": "Volumetrics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Volumetrics-io/volumetrics/issues"
  },
  "homepage": "https://github.com/Volumetrics-io/volumetrics#readme",
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdoc": "^4.0.2",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@dimforge/rapier3d": "*",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "stats.js": "^0.17.0",
    "three": "^0.151.3",
    "troika-three-text": "^0.48.0-unicode.1"
  }
}
